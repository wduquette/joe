// Sample test script

function testAssertEquals() {
    assertEquals(5, 5);
    assertEquals("abc", "abc");

    function bad() { assertEquals("abc", 5); }
    var err = catch(bad);

    assertEquals(err.message(), "Expected '5', got: 'abc'.");
}

function testFail() {
    function bad() { fail("Simulated failure"); }
    var err = catch(bad);
    assertEquals(err.message(), "Simulated failure");
}
