// type.joe.Keyword.joe
//
// Keyword tests

function testInitializer() {
    assertEQ(Keyword("abc"), #abc);
    assertEQ(Keyword("#abc"), #abc);

    assertError(\-> Keyword("a@"),
        "Expected keyword name, got: String 'a@'.");
}

function testName() {
    assertEQ(#abc.name(), "abc");
}

function testToString() {
    assertEQ(#abc.toString(), "#abc");
}
